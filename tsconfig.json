{
	"compilerOptions": {
		"target": "ES6",
		"module": "CommonJS",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"noImplicitAny": true,
		"allowJs": true,
		"outDir": "dist",
		"rootDir": "./",
		"moduleResolution": "node",
		"sourceMap": true,
	},
	"ts-node": {
		"esm": true
	}
}